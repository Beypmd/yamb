{
  "name": "discord-yamusic",
  "version": "0.0.0",
  "description": "Discord Bot for Yandex.Music",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/dvasiliev97/discord-yamusic.git"
  },
  "author": "",
  "homepage": "https://github.com/dvasiliev97/discord-yamusic",
  "bugs": {
    "url": "https://github.com/dvasiliev97/discord-yamusic/issues"
  },
  "license": "ISC",
  "main": "src/app.ts",
  "scripts": {
    "prod": "npm run build && npm run start",
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "build": "npm run clean && tsc && npm run lint:fix",
    "dev": "ts-node-dev --respawn --no-notify src/app.ts",
    "clean": "rimraf dist",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --coverage --verbose --watchAll"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "discord.js": "^11.5.1",
    "dotenv": "^8.0.0",
    "minimist": "^1.2.0",
    "node-opus": "^0.3.2",
    "pino": "^5.13.2",
    "yandex-music-api": "0.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.15",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.0.10",
    "@types/pino": "^5.8.9",
    "@types/request": "^2.48.2",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.7.1",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "pino-tee": "^0.2.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.(ts|tsx|js|jsx)": [
      "npm run lint:fix",
      "git add ."
    ]
  }
}
